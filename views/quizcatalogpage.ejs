<!doctype html>
<html lang='en'>
<head>
        <meta charset='utf-8'>
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
		<link rel="stylesheet" href="../css/bootstrap.min.css">
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
		<title>HomePage</title>
	</head>

	<% include ../partials/navbar.html %>'
	<div class="container">
	<table class="table table-hover">
    <thead>
    <tr>
      <th>#</th>
      <th>Student ID:</th>
      <th>Question</th>
      <th>Score</th>
      <th>Date:</th>
      <th>Mark:</th>
    </tr>
  </thead>
    <tbody>
    <% for(var i=0; i<quizAnswerSheet.length; i++) {%>
	 <tr>
	  <th scope="row" id="rowId"><%= i %></th>
	  <td><%= quizAnswerSheet[i].studentId %></td>
	  <td><%= quizAnswerSheet[i].total_question_num %></td>
	  <td><%= quizAnswerSheet[i].score %></td>
	  <td><%= quizAnswerSheet[i].date %></td>
    <% if (quizAnswerSheet[i].marked == false) { %>
    <td><a href="/<%= courseId %>/quiz?key=<%= quizAnswerSheet[i].key %>"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a></td>
    <% } else { %>
    <td><i class="fa fa-check" aria-hidden="true"></i></td>
    <% } %>
    </tr>
    <% } %>
  </tbody>
</table>
</div>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
	<script src="../js/bootstrap.min.js"></script>

</body>
</html>